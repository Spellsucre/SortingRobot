plot "temps_LDC.dat" using 1:2 with line title "LDC_1" lt rgb "blue"

replot "temps_AVL.dat" using 1:2 with line title "AVL_1" lt rgb "red"


replot "temps_LDC_2.dat" using 1:2 with line title "LDC_2" lt rgb "cyan"

replot "temps_AVL_2.dat" using 1:2 with line title "AVL_2" lt rgb "orange"



replot 30 with line linetype 0 title ""
replot "guid_2.dat" using 1:2 with line linetype 0 title ""

set title "Temps de calcul en fonction de la\nméthode de recherche utilisée"
set autoscale
set xrange [0 : 2200]
set xlabel "Taille de la grille"
set ylabel "Temps (s)"

replot

#saving to .png
set term png
set output "graph_LDC_AVL.png"
replot


