plot "temps_naif_2.dat" using 1:2 with line title "nbCoul = 10" lt rgb "red"

replot "temps_naif.dat" using 1:2 with line title "nbCoul = n" lt rgb "blue"

replot "temps_naif_3.dat" using 1:2 with line title "nbCoul = n^2" lt rgb "green"



replot 30 with line linetype 0 title ""
replot "guid_naif.dat" using 1:2 with line linetype 0 title ""

set title "Temps de calcul de la mÃ©thode de recherche naive"
set autoscale
set xrange [0 : 280]
set xlabel "Taille de la grille"
